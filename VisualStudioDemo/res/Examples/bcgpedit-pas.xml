<OUTLINE_DATA>
    <IgnoreOneLineBlocks>True</IgnoreOneLineBlocks>
    <IncludeSpaceLines>True</IncludeSpaceLines>
    <CaseSensitive>False</CaseSensitive>
    <WholeWords>True</WholeWords>
    <EscapeSequences>
        <EscapeSequence>\'</EscapeSequence>
        <EscapeSequence>\\</EscapeSequence>
    </EscapeSequences>
    <BLOCKS>
        <BLOCK>
            <Start>'</Start>
            <End>'</End>
            <ReplaceString>""</ReplaceString>
            <AllowNestedBlocks>False</AllowNestedBlocks>
            <Ignore>True</Ignore>
            <OneLine>True</OneLine>
        </BLOCK>
        <BLOCK>
            <Start>{</Start>
            <End>}</End>
            <ReplaceString>{... Comment ...}</ReplaceString>
            <AllowNestedBlocks>False</AllowNestedBlocks>
            <Ignore>True</Ignore>
        </BLOCK>
        <BLOCK>
            <Start>begin</Start>
            <End>end</End>
            <ReplaceString>begin ... end</ReplaceString>
            <AllowNestedBlocks>True</AllowNestedBlocks>
            <Ignore>False</Ignore>
            <KEYWORDS>
                <Keyword>procedure</Keyword>
                <Keyword>function</Keyword>
                <Keyword>for</Keyword>
                <Keyword>if</Keyword>
                <Keyword>else</Keyword>
                <Keyword>do</Keyword>
                <Keyword>while</Keyword>
                <Keyword>with</Keyword>
            </KEYWORDS>
        </BLOCK>
        <BLOCK>
            <Start>repeat</Start>
            <End>until</End>
            <ReplaceString>repeat ... until</ReplaceString>
            <AllowNestedBlocks>True</AllowNestedBlocks>
            <Ignore>False</Ignore>
        </BLOCK>
    </BLOCKS>
